config BR2_PACKAGE_OPENJDK
	bool "openjdk"
	select BR2_PACKAGE_BUILDROOT_LOCALE
	select BR2_PTHREAD_DEBUG
	depends on BR2_TOOLCHAIN_HAS_THREADS
	help
	  OpenJDK JRE runtime.

	  http://openjdk.java.net/

if BR2_PACKAGE_OPENJDK

#
# Version selection. We provide the choice between:
#
#  1. An LTS release version
#  2. The latest release version (may be the same as LTS)
#  3. A specified custom project/release/version from hg.openjdk.java.net
#

        choice
		prompt "OpenJDK version"

		config BR2_PACKAGE_OPENJDK_LTS
			bool "Latest LTS version (jdk-updates/jdk11u/jdk-11.0.1+13)"
			help
			  The current (and first) LTS version released October 2018.

		config BR2_PACKAGE_OPENJDK_LATEST
			bool "Latest version (jdk-updates/jdk11u/jdk-11.0.1+13)"
			help
			  The current version released October 2018.

		config BR2_PACKAGE_OPENJDK_CUSTOM_OJN
			bool "Specify a version from the OpenJDK Mercurial Repository"
			help
			  A custom version specified by project/release/branch from the mercurial repository hg.openjdk.java.net.

		config BR2_PACKAGE_OPENJDK_CUSTOM_TARBALL
			bool "Custom tarball"
			help
			  This option allows to specify a URL pointing to a kernel
			  source tarball. This URL can use any protocol recognized by
			  Buildroot, like http://, ftp://, file:// or scp://.

			  When pointing to a local tarball using file://, you may want
			  to use a make variable like $(TOPDIR) to reference the root of
			  the Buildroot tree.

		config BR2_PACKAGE_OPENJDK_CUSTOM_GIT
			bool "Custom Git repository"
			help
			  This option allows Buildroot to get the Linux kernel source
			  code from a Git repository.

		config BR2_PACKAGE_OPENJDK_CUSTOM_HG
			bool "Custom Mercurial repository"
			help
			  This option allows Buildroot to get the Linux kernel source
			  code from a Mercurial repository.

		config BR2_PACKAGE_OPENJDK_CUSTOM_SVN
			bool "Custom Subversion repository"
			help
			  This option allows Buildroot to get the Linux kernel source
			  code from a Subversion repository.

	endchoice

	
	if BR2_PACKAGE_OPENJDK_CUSTOM_OJN

		config BR2_PACKAGE_OPENJDK_CUSTOM_OJN_PROJECT
			string "OpenJDK Project"
			default "jdk-updates"
			help
			  The top branch at hg.openjdk.java.net (e.g. jdk-updates)

		config BR2_PACKAGE_OPENJDK_CUSTOM_OJN_RELEASE
			string "OpenJDK Release"
			default "jdk11u"
			help
			  The second-from-top branch at hg.openjdk.java.net (e.g. jdk11u)

		config BR2_PACKAGE_OPENJDK_CUSTOM_OJN_VERSION
			string "OpenJDK Release"
			default "jdk-11.0.1+13"
			help
			  The complete version title at hg.openjdk.java.net (e.g. jdk-11.0.1+13)
	endif

	config BR2_PACKAGE_OPENJDK_CUSTOM_TARBALL_LOCATION
		string "URL of custom kernel tarball"
		depends on BR2_PACKAGE_OPENJDK_CUSTOM_TARBALL

	if BR2_PACKAGE_OPENJDK_CUSTOM_GIT || BR2_PACKAGE_OPENJDK_CUSTOM_HG || BR2_PACKAGE_OPENJDK_CUSTOM_SVN

		config BR2_PACKAGE_OPENJDK_CUSTOM_REPO_URL
			string "URL of custom repository"

		config BR2_PACKAGE_OPENJDK_CUSTOM_REPO_VERSION
			string "Custom repository version"
			help
			  Revision to use in the typical format used by
			  Git/Mercurial/Subversion E.G. a sha id, a tag, branch, ..

	endif

#
# Build type selection. We provide the choice between:
#
#  1. JRE
#  2. JDK
#
        choice
		prompt "Build type"

	config BR2_PACKAGE_OPENJDK_JRE
		bool "JRE"
		help
		  Build only the JRE to run java applications.

	config BR2_PACKAGE_OPENJDK_JDK
		bool "JDK"
		help
		  Build the JDK to compile, link, debug, and run java code and applications.

	endchoice

#
# Build type selection. We provide the choice between:
#
#  1. Headless
#  2. WITH_X
#
        choice
		prompt "Graphical Interface"

	config BR2_PACKAGE_OPENJDK_WITH_X
		bool "With X"
		comment "needs an X.Org server"
		depends on BR2_PACKAGE_XORG7
		help
		  Build against X.Org to run graphical applications.

	config BR2_PACKAGE_OPENJDK_HEADLESS
		bool "Without X"
		help
		  Build without graphical output.

	endchoice



